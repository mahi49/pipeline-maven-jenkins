pipeline {
    agent any

    stages {
          stage('checkout') {
            steps {
            checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'mahender', url: 'https://github.com/mahi49/pipeline-maven-jenkins.git']]])}
        }
         stage('validate') {
            sh ' mvn validate'
            }
        }
          stage('compile') {
            steps {
             sh 'mvn compile'
            }
        } 
        stage('test') {
            steps {
            sh 'mvn test'
            }
        }
        stage('package ') {
            steps {
            sh 'mvn package'
            }
        }
        stage('hello') {
            steps { 
            sh 'mvn hello'
            }
        }
        stage('world') {
            steps { 
            sh 'world'
            }
            
        }
        stage('world-hello') {
            steps { 
            sh 'world'
            }
        }
        stage('new-helloww') {
            steps { 
            sh 'world'
            }
        }    
    }
}
