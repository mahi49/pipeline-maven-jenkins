pipeline {
    agent any

    stages {
          stage('checkout') {
            steps {
            checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'mahender', url: 'https://github.com/mahi49/maven-pipeline.git']]])
            }
        }
         stage('validate') {
            steps {
            sh ' mvn validate'
            }
        }
          stage('compile') {
            steps {
             sh 'mvn compile'
            }
        } 
        stage('test') {
            steps {
            sh 'mvn test'
            }
        }
        stage('package ') {
            steps {
            sh 'mvn package'
            }
            
        }
        
    }
}
